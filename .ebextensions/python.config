option_settings:
  "aws:elasticbeanstalk:container:python":
    WSGIPath: app:app
files:
  "/opt/elasticbeanstalk/hooks/appdeploy/pre/01_download_resources.sh":
    mode: "000755"
    owner: root
    group: root
    content: |
      #!/usr/bin/env bash
      aws s3 cp s3://bucket-api-2/texto.txt /var/app/ondeck/
container_commands:
  01_move_file:
    command: "mv /var/app/ondeck/texto.txt /var/app/current/texto.txt"
